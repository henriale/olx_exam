{
  "name": "henriale/olx",
  "description": "OLX exam project",
  "keywords": ["exam"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "php-di/php-di": "5.4.0",
    "symfony/http-foundation": "3.1.x-dev",
    "nilportugues/sql-query-builder": "1.5.0"
  },
  "require-dev": {
    "symfony/var-dumper": "3.1.x-dev"
  },
  "autoload": {
    "psr-4": {
      "": "modules/",
      "App\\": "app/"
    },
    "classmap": []
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  }
}
